package exercicio;
import java.util.Scanner;
public class exercicico_14 {
	public static void main(String[]args) {
	Scanner sc=new Scanner(System.in);
	int anotual=0;
	int nascimento=0;
	int idade=0;
	System.out.println("digite o ano que estamos");
	anotual=sc.nextInt();
	System.out.println("digite o ano que nasceu");
	nascimento=sc.nextInt();
	System.out.println(anotual-nascimento);
	if(anotual>=16) {
		System.out.println("Pode votar");
	}else {
		System.out.println("NÃ£o pode votar");
	}
	}

}
